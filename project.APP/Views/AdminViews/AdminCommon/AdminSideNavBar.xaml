<?xml version="1.0" encoding="utf-8" ?>
<VerticalStackLayout xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
                     xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
                     x:Class="project.App.Views.AdminViews.AdminSideNavBar">

    <Button TextColor="White" Background="Transparent" FontSize="25" HorizontalOptions="FillAndExpand" HeightRequest="80" 
            Text="teachers" Command="{Binding GoToTeacherCommand}"/>
    <Button TextColor="White" Background="#6E6E6E" FontSize="25" HorizontalOptions="FillAndExpand" HeightRequest="80" 
            Text="students" Command="{Binding GoToStudentCommand}" />
    <Button TextColor="White" Background="Transparent" FontSize="25" HorizontalOptions="FillAndExpand" HeightRequest="80" 
            Text="subjects" Command="{Binding GoToSubjectCommand}"/>

</VerticalStackLayout>
